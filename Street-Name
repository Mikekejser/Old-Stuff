	letter = ['Curious', 'Cordial', 'Cool-headed', 'Considerate', 'Confused', 'Cold', 'Confident', 'Competent', 'Clumsy', 'Clean', 'Cheerful', 'Charming', 'Cautious', 'Careful', 'Creative', 'Clever', 'Compassionate', 'Courageous', 'Courteous', 'Calm', 'Charismatic']
