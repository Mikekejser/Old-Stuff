import random

name = input("What's your name: ")
first_letter = name[0].lower()

if first_letter == 'a':
	letter = ['Awesome', 'Adventurous', 'Ambitious', 'Arrogant', 'Above Average', 'Agreeable', 'Amazing', 'Amusing', 'Analytical', 'Artificial', 'Artistic', 'Average']
elif first_letter == 'b':
	letter = ['Balanced', 'Beautiful', 'Below Average', 'Brave', 'Brilliant', 'Buff']
