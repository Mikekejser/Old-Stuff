	letter = ['Diligent', 'Deep', 'Delicate', 'Delightful', 'Devoted', 'Dirty', 'Disagreeable', 'Discreet', 'Dramatic', 'Drunk', 'Dangerous', 'Distrustful']
elif first_letter == 'e':
	letter = ['Empathetic', 'Eccentric', 'Eager', 'Efficient', 'Egoist', 'Enthusiastic', 'Excellent', 'Evasive']
elif first_letter == 'f':
	letter = ['Flamboyant', 'Flexible', 'Fabulous', 'Ferocious', 'Fiery', 'Funny', 'Friendly']
elif first_letter == 'g':
	letter = ['Generous', 'Gentle', 'Great', 'Groggy', 'Gangsta']
